
public class Aluno {

	
	private String name;
	private int	id;
	private int nota;
	private int materia;
	private int presenca;
	private Turma turma;
	
	public Aluno(String name, int id, int nota, int materia, int presenca, Turma turma) {
		
		this.name = name;
		this.id = id;
		this.nota = nota;
		this.materia = materia;
		this.presenca = presenca;
		this.turma = turma;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getNota() {
		return nota;
	}

	public void setNota(int nota) {
		this.nota = nota;
	}

	public int getMateria() {
		return materia;
	}

	public void setMateria(int materia) {
		this.materia = materia;
	}

	public int getPresenca() {
		return presenca;
	}

	public void setPresenca(int presenca) {
		this.presenca = presenca;
	}

	public Turma getTurma() {
		return turma;
	}

	public void setTurma(Turma turma) {
		this.turma = turma;
	}
	
	
}