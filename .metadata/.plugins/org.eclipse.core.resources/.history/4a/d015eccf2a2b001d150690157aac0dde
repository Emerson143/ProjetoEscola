
public class Aluno {

	
	private String name;
	private int	id;
	private int nota;
	
	public Aluno(String name, int id, int nota) {
	
		this.name = name;
		this.id = id;
		this.nota = nota;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getNota() {
		return nota;
	}

	
	
	
	
}
